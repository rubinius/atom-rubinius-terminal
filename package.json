{
  "name": "rubinius-terminal",
  "main": "./lib/rubinius-terminal",
  "version": "0.0.0",
  "description": "Opens a terminal tab within Atom that is configured to run Rubinius",
  "activationCommands": {
    "atom-workspace": "rubinius-terminal:open",
    "atom-workspace": "rubinius-terminal:open-split-right",
    "atom-workspace": "rubinius-terminal:open-split-left",
    "atom-workspace": "rubinius-terminal:open-split-top",
    "atom-workspace": "rubinius-terminal:open-split-bottom"
  },
  "repository": "https://github.com/rubinius/atom-rubinius-terminal",
  "license": "MPL/2.0",
  "engines": {
    "atom": ">=0.179.0 <2.0.0"
  },
  "dependencies": {
    "debounce": "0.0.3",
    "fs-plus": "^2.2.3",
    "keypather": "^1.3.2"
  }
}
