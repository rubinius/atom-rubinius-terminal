{
  "name": "rubinius-terminal",
  "main": "./lib/rubinius-terminal",
  "version": "0.0.1",
  "description": "Opens a terminal tab within Atom that is configured to run Rubinius",
  "activationCommands": {
    "atom-workspace": [
      "rubinius-terminal:open",
      "rubinius-terminal:split-left",
      "rubinius-terminal:split-right",
      "rubinius-terminal:split-top",
      "rubinius-terminal:split-bottom"
    ]
  },
  "repository": "https://github.com/rubinius/atom-rubinius-terminal",
  "license": "MPL/2.0",
  "engines": {
    "atom": ">=0.179.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "debounce": "0.0.3",
    "fs-plus": "^2.2.3",
    "keypather": "^1.3.2"
  }
}
